{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/index.ts",
    "src/deploy-commands.ts"
  ],
  "project": [
    "src/**/*.ts"
  ],
  "ignore": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts"
  ],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "unlisted": "error",
    "exports": "error",
    "nsExports": "error",
    "classMembers": "warn",
    "types": "warn",
    "nsTypes": "warn",
    "enumMembers": "warn",
    "duplicates": "error"
  },
  "exclude": [
    "unresolved"
  ]
}